{
	"private": true,
	"name": "mastodon-archive-search",
	"version": "1.5.1",
	"description": "a basic web ui for searching your own posts from an exported archive",
	"author": "Sean S. LeBlanc <sean.s.leblanc@gmail.com>",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": "24.x"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/seleb/mastodon-archive-search.git"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/npm",
			[
				"@semantic-release/github",
				{
					"successComment": false,
					"failTitle": false
				}
			],
			[
				"@semantic-release/git",
				{
					"assets": [
						"docs/**/*",
						"CHANGELOG.md",
						"package.json",
						"package-lock.json"
					]
				}
			]
		]
	},
	"scripts": {
		"postversion": "npm run build",
		"start": "vite",
		"build": "vite build",
		"serve": "vite preview --port 80",
		"start:prod": "npm run build && npm run serve",
		"lint": "eslint \"**/*.{js,ts}\"",
		"typecheck": "tsc --skipLibCheck",
		"analyze": "npx vite-bundle-visualizer -t sunburst"
	},
	"devDependencies": {
		"@types/js-search": "^1.4.4",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"eslint": "^8.57.1",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.32.0",
		"js-search": "^2.0.1",
		"localforage": "^1.10.0",
		"nanoid": "^5.0.9",
		"sass": "^1.71.0",
		"typescript": "^5.0.4",
		"vite": "^5.0.4"
	}
}
